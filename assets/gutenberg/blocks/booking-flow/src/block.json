{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpappointments/booking-flow",
	"category": "wpappointments",
	"title": "Booking Flow",
	"attributes": {
		"flowType": {
			"type": "string",
			"default": "OneStep"
		},
		"alignment": {
			"type": "string",
			"default": "Left"
		},
		"width": {
			"type": "string",
			"default": "Narrow"
		},
		"trimUnavailable": {
			"type": "boolean",
			"default": true
		}
	},
	"textdomain": "wpappointments",
	"editorScript": "file:../../../../../build/booking-flow-block.tsx.js",
	"editorStyle": "file:./index.css",
	"style": [
		"file:./style-index.css",
		"file:../../../../../build/frontend.tsx.css",
		"wp-components"
	],
	"viewScript": [
		"file:../../../../../build/booking-flow-block-view.tsx.js",
		"react",
		"react-dom",
		"wp-components"
	],
	"render": "file:./render.php"
}
